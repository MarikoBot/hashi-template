{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "semicolon": [true, "always"],
    "no-console": false,
    "max-classes-per-file": [true, 1],
    "no-redundant-jsdoc": true,
    "no-eval": false,
    "no-string-literal": true,
    "no-namespace": true,
    "no-angle-bracket-type-assertion": false
  }
}
