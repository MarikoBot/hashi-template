{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": "error",
    "no-console": false,
    "max-classes-per-file": [true, 1],
    "no-redundant-jsdoc": true,
    "no-eval": false,
    "no-string-literal": false,
    "no-namespace": false
  }
}
